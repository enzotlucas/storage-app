@model Storage.App.MVC.Core.Enterprise.EnterpriseEntity

@{
    ViewData["Title"] = "Detalhes da Empresa - Serviço de estoque e vendas";
}

<h1>Detalhes da Empresa</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
</div>
<hr />
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Editar</a>
    <a class="btn btn-secondary" asp-action="Index">Voltar para lista</a>
</div>
